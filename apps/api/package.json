{
  "dependencies": {
    "@fastify/ajv-compiler": "^4.0.5",
    "@fastify/helmet": "^13.0.2",
    "@fastify/sensible": "^6.0.3",
    "@sinclair/typebox": "^0.34.41",
    "dotenv": "^17.2.3",
    "fastify": "^5.6.1",
    "fastify-plugin": "^5.1.0",
    "pino": "^10.1.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@eslint/js": "9.38.0",
    "@fastify/type-provider-typebox": "^6.1.0",
    "@powercoach/config": "workspace:*",
    "@types/node": "^24.9.2",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "@vitest/coverage-v8": "^4.0.6",
    "eslint": "9.38.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "pino-pretty": "^13.1.2",
    "prettier": "^3.6.2",
    "tsup": "^8.5.0",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "vitest": "^4.0.6"
  },
  "engines": {
    "node": "20"
  },
  "files": ["dist", "node_modules"],
  "main": "dist/index.js",
  "name": "@powercoach/api",
  "private": true,
  "scripts": {
    "build": "tsup --config tsup.config.js",
    "ci": "pnpm lint && pnpm format && pnpm typecheck && pnpm test && pnpm build",
    "dev": "tsx watch --tsconfig tsconfig.src.json src/index.ts",
    "format": "prettier . --check",
    "format:write": "prettier . --write",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest run --coverage --reporter=verbose",
    "test:watch": "vitest",
    "typecheck": "pnpm run typecheck:src && pnpm run typecheck:test",
    "typecheck:src": "tsc --project tsconfig.src.json --noEmit",
    "typecheck:test": "tsc --project tsconfig.test.json --noEmit"
  },
  "type": "module",
  "version": "0.1.0"
}
