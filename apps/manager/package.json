{
  "dependencies": {
    "react": "^19.2.0",
    "react-dom": "^19.2.0"
  },
  "devDependencies": {
    "@powercoach/config": "workspace:*",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@typescript-eslint/eslint-plugin": "^8.46.3",
    "@typescript-eslint/parser": "^8.46.3",
    "@vitejs/plugin-react": "^5.1.0",
    "@vitest/coverage-v8": "^4.0.8",
    "dotenv": "^17.2.3",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "jsdom": "^27.1.0",
    "prettier": "^3.6.2",
    "typescript": "^5.9.3",
    "vercel": "^48.9.0",
    "vite": "^7.2.2",
    "vitest": "^4.0.8"
  },
  "engines": {
    "node": "20"
  },
  "main": "src/main.tsx",
  "name": "@powercoach/manager",
  "private": true,
  "scripts": {
    "build": "vite build && pnpm vercel:prepare",
    "ci": "pnpm lint && pnpm format && pnpm typecheck && pnpm test && pnpm build",
    "dev": "vite dev",
    "format": "prettier . --check",
    "format:write": "prettier . --write",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "preview": "vite preview",
    "test": "vitest run --coverage --reporter=verbose",
    "test:watch": "vitest",
    "typecheck": "pnpm run typecheck:src && pnpm run typecheck:test",
    "typecheck:src": "tsc --project tsconfig.src.json --noEmit",
    "typecheck:test": "tsc --project tsconfig.test.json --noEmit",
    "vercel:prepare": "tsx scripts/vercelPrepare.ts"
  },
  "type": "module",
  "version": "0.1.0"
}
