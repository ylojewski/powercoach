{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "builds": [
    { "src": "**/*", "use": "@vercel/static" }
  ],
  "framework": null,
  "rewrites": [
    { "source":"/api/:path*", "destination": "${VITE_API_BASE_URL}/:path*" },
    { "source": "/(.*)",  "destination": "/index.html" }
  ]
}
